{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./built",
    "noImplicitAny": true,
    "allowJs": true,
    "listFiles": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "./server/**/*.ts"
  ]
}